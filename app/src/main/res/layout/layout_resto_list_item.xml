<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <edu.ateneo.cie199.bucketdish.CustomTextView
        android:id="@+id/txv_restoname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:text="TextView"
        android:textSize="18sp" />

    <CheckBox
        android:id="@+id/chk_bucket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/txv_restoname"
        android:layout_marginEnd="65dp"
        android:buttonTint="@color/cast_libraries_material_featurehighlight_text_body_color"
        android:checked="false"
        android:text="BucketDished"
        android:textColor="@color/cast_libraries_material_featurehighlight_text_body_color" />
</RelativeLayout>